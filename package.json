    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && yarn run start",
    "db:reset": "node/server/db/seeds/index.js && node server.js",
